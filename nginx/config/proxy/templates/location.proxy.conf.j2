location / {
  try_files $uri $uri/ @{{ nginx_config_proxy_config_name }};
{% if nginx_config_proxy_root is defined and nginx_config_proxy_root != "" %}
  root {{ nginx_config_proxy_root }};
{% endif %}
}

location @{{ nginx_config_proxy_config_name }} {
  proxy_pass http://@{{ nginx_config_proxy_upstream }};
}
